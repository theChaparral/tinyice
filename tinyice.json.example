{
    "bind_host": "0.0.0.0",
    "port": "8000",
    "hostname": "radio.example.com",
    "base_url": "https://radio.example.com",
    "page_title": "TinyIce ❄️",
    "page_subtitle": "Live Streaming Server powered by Go",
    "use_https": false,
    "auto_https": false,
    "https_port": "443",
    "acme_email": "admin@example.com",
    "domains": ["radio.example.com"],
    "max_listeners": 100,
    "directory_listing": false,
    "directory_server": "http://dir.xiph.org/cgi-bin/yp-cgi",
    "low_latency_mode": false,
    "banned_ips": [],
    "relays": [
        {
            "url": "http://remote-server:8000/live",
            "mount": "/relay",
            "password": "relay-password",
            "burst_size": 65536,
            "enabled": false
        }
    ],
    "fallback_mounts": {
        "/live": "/backup"
    },
    "visible_mounts": {
        "/live": true
    }
}
